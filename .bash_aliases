alias update='sudo apt update && sudo apt upgrade -y'
alias clean='sudo apt autoremove && sudo apt autoclean'
alias ..='cd ..;pwd'
alias rm='rm -i'
alias cat='cat -n'
alias mkdir='mkdir -v'
alias rmdir='rmdir -v'
alias c='clear'
alias config_shell='$EDITOR $HOME/.zshrc'
alias config_terminal='$EDITOR $HOME/.config/alacritty/alacritty.yml'
alias config_vim='$EDITOR $HOME/.vimrc'
